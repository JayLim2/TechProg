<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="424.0" prefWidth="646.0" styleClass="main" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="samara.university.client.controllers.MainFormController">
    <children>
        <Button layoutX="434.0" layoutY="226.0" mnemonicParsing="false" onAction="#startGameAction" prefHeight="51.0"
                prefWidth="173.0" text="Старт">
            <font>
                <Font size="24.0"/>
            </font>
        </Button>
        <Button layoutX="434.0" layoutY="287.0" mnemonicParsing="false" onAction="#helpAction" prefHeight="51.0"
                prefWidth="173.0" text="Справка">
            <font>
                <Font size="24.0"/>
            </font>
        </Button>
        <Button layoutX="434.0" layoutY="346.0" mnemonicParsing="false" onAction="#exitApplicationAction"
                prefHeight="51.0" prefWidth="173.0" text="Выход">
            <font>
                <Font size="24.0"/>
            </font>
        </Button>
    </children>
</AnchorPane>
