<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>
<AnchorPane prefHeight="264.0" prefWidth="447.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="samara.university.client.controllers.SellFormController">
    <children>
        <Button layoutX="257.0" layoutY="199.0" mnemonicParsing="false" onAction="#cancel" prefHeight="39.0"
                prefWidth="116.0" text="Отмена">
            <font>
                <Font size="18.0"/>
            </font>
        </Button>
        <Button layoutX="96.0" layoutY="199.0" mnemonicParsing="false" onAction="#ok" prefHeight="39.0"
                prefWidth="110.0" text="ОК">
            <font>
                <Font size="18.0"/>
            </font>
        </Button>
        <Text layoutX="73.0" layoutY="128.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Цена:">
            <font>
                <Font size="18.0"/>
            </font>
        </Text>
        <Text layoutX="73.0" layoutY="98.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Количество:">
            <font>
                <Font size="18.0"/>
            </font>
        </Text>
        <Text layoutX="155.0" layoutY="50.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Продажа ЕГП">
            <font>
                <Font size="24.0"/>
            </font>
        </Text>
        <Spinner fx:id="spinnerCount" layoutX="273.0" layoutY="78.0" prefHeight="25.0" prefWidth="100.0"/>
        <Spinner fx:id="spinnerPrice" layoutX="273.0" layoutY="108.0" prefHeight="25.0" prefWidth="100.0"/>
    </children>
</AnchorPane>
