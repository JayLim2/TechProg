<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controllers.MainFormController">
    <children>
        <Text layoutX="134.0" layoutY="104.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Игра &quot;Менеджмент&quot;">
            <font>
                <Font size="40.0"/>
            </font>
        </Text>
        <Button layoutX="214.0" layoutY="200.0" mnemonicParsing="false" onAction="#startGameAction" prefHeight="51.0"
                prefWidth="173.0" text="Старт">
